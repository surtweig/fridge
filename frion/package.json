{
    "name": "frion",
    "displayName": "Frion",
    "description": "Fridge Frion Language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "frion",
            "aliases": ["Frion", "frion"],
